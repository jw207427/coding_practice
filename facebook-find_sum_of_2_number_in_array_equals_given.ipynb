   "source": [
    "def find_sum_given(A, x):\n",
    "    if not A or len(A)==0:\n",
    "        return\n",
    "    \n",
    "    if not x:\n",
    "        return\n",
    "    \n",
    "    for a in A:\n",
    "        pair = x-a\n",
    "        if pair in A:\n",
    "            return (a, pair)\n",
    "        \n",
    "    return\n",
    "\n",
    "A = [3, 6, 8, 3, -2,0]\n",
    "\n",
    "x =6\n",
    "\n",
    "find_sum_given(A, x)"
   ]

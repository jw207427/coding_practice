    "#O(n^2)\n",
    "A = [1, 2, 3, 4, 5]\n",
    "\n",
    "\n",
    "def print_subset(A):\n",
    "    if not A:\n",
    "        return\n",
    "    print([])\n",
    "    for i in range(len(A)):\n",
    "        for j in range(i+1, len(A)+1):\n",
    "            print(A[i:j])\n",
    "            \n",
    "print_subset(A)"
   ]

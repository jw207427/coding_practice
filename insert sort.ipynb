    "def insertion_sort(A):\n",
    "    for i in range(1,len(A)):\n",
    "        cur = A[i]\n",
    "        pos = i\n",
    "\n",
    "        while pos>0 and A[pos-1]>cur:\n",
    "            A[pos]=A[pos-1]\n",
    "            pos = pos-1\n",
    "\n",
    "        A[pos]=cur\n",
    "    \n",
    "    return A\n",
    "\n",
    "A = [6, 5, 3,1,8,7,2, 4]\n",
    "insertion_sort(A)\n",
    "# print(A)"
   ]
